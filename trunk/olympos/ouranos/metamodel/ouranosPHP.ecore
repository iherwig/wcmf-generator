<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="metamodel"
    nsURI="http://www.example.org/metamodel" nsPrefix="metamodel">
  <eClassifiers xsi:type="ecore:EClass" name="OModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="types" upperBound="-1"
        eType="#//OType" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" upperBound="-1" eType="#//directory"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OType" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PHPDatatype" eSuperTypes="#//OType"/>
  <eClassifiers xsi:type="ecore:EClass" name="Class" eSuperTypes="#//OType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="features" upperBound="-1"
        eType="#//classvariable" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" eType="#//PHPfunction" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="classvariable">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//OType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="directory" eSuperTypes="#//OType">
    <eStructuralFeatures xsi:type="ecore:EReference" eType="#//Class" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PHPfunction"/>
  <eClassifiers xsi:type="ecore:EClass" name="var" eSuperTypes="#//PHPDatatype"/>
  <eClassifiers xsi:type="ecore:EClass" name="phpSourceText">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="source"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="fileName"/>
  </eClassifiers>
</ecore:EPackage>
