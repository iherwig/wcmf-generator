import uml;

extension common::umlutils;

extension cartridge::ChiCmf::extensions::globals;

/**
 * Get the package name of the class
 */
String getFullPackageName(uml::PackageableElement cls) :
	cls.getNearestPackage()!=null ? getPackageName(cls.getNearestPackage()).replaceFirst(RootPackage()+".", "") : "";
