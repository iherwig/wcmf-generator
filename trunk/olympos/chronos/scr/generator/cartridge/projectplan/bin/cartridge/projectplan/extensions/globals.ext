String TECHNICAL_TYPE(): GLOBALVAR technicalType;
String ENVIRONMENTAL_TYPE(): GLOBALVAR environmentalType;
String COUNTER_USE_CASE(): "counter-useCase";
String projectStartDate(): GLOBALVAR projectStartDate;
String projectFinishDate(): GLOBALVAR projectFinishDate;
int ACTION_COUNT_SIMPLE(): GLOBALVAR actionCountSimple;
int ACTION_COUNT_AVERAGE(): GLOBALVAR actionCountAverage;