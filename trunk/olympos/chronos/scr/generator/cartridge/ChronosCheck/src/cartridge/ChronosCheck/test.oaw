<workflow>
	<!-- metamodel setup -->
	<bean class="oaw.uml2.Setup" standardUML2Setup="true"/>

	<bean id="uml2" class="org.openarchitectureware.uml2.UML2MetaModel"/>
	
	<bean id="Chronos" class="org.openarchitectureware.uml2.profile.ProfileMetaModel">
		<profile value="/home/nist/Desktop/project-ea/chronos.profile.uml"/>
	</bean>
	
	<bean id="Ouranos" class="org.openarchitectureware.uml2.profile.ProfileMetaModel">
		<profile value="/home/nist/Desktop/project-ea/ouranos.profile.uml"/>
	</bean>

	<component class="oaw.emf.XmiReader">
		<modelFile value="/home/nist/Desktop/project-ea/project-part.uml" />
		<outputSlot value="localModel" />
	</component>
	
	<property name="rootPackage" value="" />
	<property name="libraryPackage" value="" />
	<property name="requiredControllerSuperclass" value="" />
	<property name="requiredNodeSuperclass" value="" />
	
	<component file="cartridge/ChronosCheck/checkAll.oaw" inheritAll="true">
		<uml2 idRef="uml2"/>
		<Chronos idRef="Chronos"/>
		<Ouranos idRef="Ouranos"/>
		<modelToCheck value="localModel"/>
	</component>
</workflow>