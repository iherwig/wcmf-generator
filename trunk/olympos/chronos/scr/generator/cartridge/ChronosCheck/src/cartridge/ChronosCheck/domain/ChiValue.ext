import uml;
import Chronos;

extension common::util;

boolean hasName(Chronos::ChiValue this):
	!this.name.isNullOrEmpty()
;

boolean hasValidName(Chronos::ChiValue this):
	switch(this.name) {
		case "type": false
		case "state": false
		case "oid": false
		case "value": false
		case "none": false
		case "sortkey": false
		case "null": false
		case "Null": false
		case "NULL": false
		default: true
	}
;
