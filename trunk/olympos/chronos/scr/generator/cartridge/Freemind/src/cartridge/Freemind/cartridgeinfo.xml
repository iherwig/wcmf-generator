<?xml version="1.0" encoding="UTF-8"?>
<!-- Defines if this carrtridge is part of the basic ChiGen-->
<cartridgeinfo system="false">
<!-- the name of the cartridge, will appear in the web front end -->
	<name>FreeMind</name>
<!-- a verbose description of the cartridge-->
	<description>
		this cartridge generates a FreeMind file from the Model. 
	</description>
<!-- additional cartridges required by this-->	
	<dependencies>
		<dependency>ChronosCommon</dependency>
	</dependencies>
	<properties>
		<!--codeGenerator: true will appear in the list of available generators in the CWM -->	
		<property name="codeGenerator">true</property>
			<!--Name of the file that contains the workflow -->	
		<property name="workflow">workflow/freeMind.oaw</property>
		<property name="targetPlatform">FreeMind</property>
	</properties>
</cartridgeinfo>