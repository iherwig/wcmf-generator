/**
 * Check if required Node base class exists
 */
context Model ERROR
	"Required Node superclass '"+RequiredNodeSuperClass()+"' does not exist in the model" :
	let requiredSuperClass = this.allOwnedElements().typeSelect(WCMFNode).select(e|e.qualifiedName == RequiredNodeSuperClass()) :
 		requiredSuperClass.size != 0
;
/**
 * Check if required Controller base class exists
 */
context Model ERROR
	"Required Controller superclass '"+RequiredControllerSuperClass()+"' does not exist in the model" :
	let requiredSuperClass = this.allOwnedElements().typeSelect(WCMFController).select(e|e.qualifiedName == RequiredControllerSuperClass()) :
 		requiredSuperClass.size != 0
;

