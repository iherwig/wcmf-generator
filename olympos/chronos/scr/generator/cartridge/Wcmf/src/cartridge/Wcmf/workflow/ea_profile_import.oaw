<?xml version="1.0" encoding="UTF-8"?>
<workflow>

	<!-- Export the Enterprise Architect profile to UML format -->
	<component class="oaw.uml2.toolsupport.ea.EA_Xmi2ProfileExporter" >
		<EapFile value="${profileEaFile}" />
		<ModelFile value="${profileUmlFile}" />
		<PackageName value="${profilePkg}" />
		<Cleanup value="true"/>
	</component>

</workflow>